{
  "extends": "@tsconfig/node18/tsconfig.json",
  "display": "Node 18",
  "compilerOptions": {
    "lib": [
      "es2022"
    ],
    "module": "commonjs",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}